'use-strict'
$(document).ready ->

	# ONE PAGE SCROLL
	$(".onepage-scroll-content").onepage_scroll({
		sectionContainer: ".onepage-scroll-section"
		easing: "ease"
		animationTime: 1000
		pagination: true
		updateURL: false
		loop: false
		keyboard: true
		responsiveFallback: false
		direction: "vertical"
		})

	return
















